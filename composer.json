{
  "name": "procket/procket",
  "type": "project",
  "description": "The Procket Application Skeleton.",
  "keywords": [
    "procket application",
    "procket framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "procket/framework": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
