{
  "name": "pocket/app",
  "description": "The Pocket Application.",
  "keywords": [
    "pocket application",
    "pocket framework"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "pocket/framework": "@dev"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
